generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Camp {
  id       BigInt  @id(map: "image-gallery_Camp_pkey") @default(autoincrement())
  imageScr String?
  tags     String?
}

model Farsang {
  id         BigInt    @id(map: "image-gallery_pkey") @default(autoincrement())
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  imageScr   String?
  tags       String?
}

model KNT {
  id       BigInt  @id @default(autoincrement())
  imageScr String?
  tags     String?
}

model Locsol__B_l {
  id       BigInt  @id
  imageScr String?
  tags     String?

  @@map("Locsoló_Bál")
}

model May_Portya {
  id       BigInt  @id
  imageScr String?
  tags     String?
}
